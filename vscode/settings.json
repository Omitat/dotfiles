// Place your settings in this file to overwrite the default settings
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/assets/js/build": true,
    "**/stats.json": true,
    "**/.next": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/build": true,
    "**/dist": true,
    "**/infrastructure/environment/.terraform": true,
    "**/infrastructure/vendor": true
  },
  "files.watcherExclude": {
    "**/.DS_Store": true,
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/build": true,
    "**/dist": true,
    "**/assets/js/build": true,
    "**/stats.json": true,
    "**/.next": true
  },
  "editor.tabSize": 2,
  "editor.fontFamily": "Operator Mono",
  "editor.rulers": [100],
  "editor.wordWrap": "on",
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.renderWhitespace": "boundary",
  "editor.renderIndentGuides": true,
  "editor.cursorBlinking": "phase",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.fontFamily": "Operator Mono",
  "terminal.integrated.fontSize": 11,
  "window.openFoldersInNewWindow": "on",
  "window.reopenFolders": "all",
  "color-highlight.markerType": "dot-after",
  "auto-close-tag.SublimeText3Mode": true,
  "projectManager.vscode.baseFolders": [
    "/Users/tmorris/Development",
    "/Users/tmorris/Projects"
  ],
  "projectManager.vscode.ignoredFolders": [
    "node_modules",
    "out",
    "typings",
    "test",
    "build",
    "dist"
  ],
  "projectManager.vscode.maxDepthRecursion": 1,
  "projectManager.git.baseFolders": [
    "/Users/tmorris/Development",
    "/Users/tmorris/Projects"
  ],
  "projectManager.git.ignoredFolders": [
    "node_modules",
    "out",
    "typings",
    "test",
    "build",
    "dist"
  ],
  "projectManager.git.maxDepthRecursion": 1,
  "projectManager.cacheProjectsBetweenSessions": true,
  "files.associations": {
    "*.html": "handlebars"
  },
  "workbench.colorTheme": "Snazzy Operator",
  "workbench.iconTheme": "material-icon-theme",
  "typescript.check.npmIsInstalled": false,
  "git.enableSmartCommit": true,
  "window.zoomLevel": 0,
